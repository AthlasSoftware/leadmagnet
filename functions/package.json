{
  "name": "athlas-leadmagnet-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for Athlas Lead Magnet",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1",
    "helmet": "^7.0.0",
    "nodemailer": "^6.9.4",
    "pdf-lib": "^1.17.1",
    "rate-limiter-flexible": "^2.4.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/nodemailer": "^6.4.9",
    "@types/validator": "^13.11.1",
    "typescript": "^5.1.6"
  },
  "private": true
}
